{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "google_auth": false,
  "disabled_mcps": [
    "websearch_exa"
  ],
  "claude_code": {
    "mcp": true,
    "commands": true,
    "skills": true,
    "agents": true,
    "hooks": true
  },
  "lsp": {
    "typescript-language-server": {
      "command": [
        "typescript-language-server",
        "--stdio"
      ],
      "extensions": [
        ".ts",
        ".tsx",
        ".js",
        ".jsx"
      ]
    },
    "pyright": {
      "command": [
        "pyright-langserver",
        "--stdio"
      ],
      "extensions": [
        ".py"
      ]
    },
    "gopls": {
      "command": [
        "gopls"
      ],
      "extensions": [
        ".go"
      ]
    }
  },
  "agents": {
    "sisyphus": {
      "model": "openai/gpt-5.3-codex",
      "reasoningEffort": "high"
    },
    "oracle": {
      "model": "openai/gpt-5.3-codex",
      "reasoningEffort": "high"
    },
    "librarian": {
      "model": "openai/gpt-5.3-codex-spark",
      "reasoningEffort": "low"
    },
    "explore": {
      "model": "openai/gpt-5.3-codex-spark",
      "reasoningEffort": "low"
    },
    "frontend-ui-ux-engineer": {
      "model": "anthropic/claude-opus-4-6",
      "reasoningEffort": "high"
    },
    "document-writer": {
      "model": "openai/gpt-5.3-codex-spark",
      "reasoningEffort": "low"
    },
    "multimodal-looker": {
      "model": "openai/gpt-5.3-codex",
      "reasoningEffort": "low"
    },
    "prometheus": {
      "model": "openai/gpt-5.3-codex",
      "reasoningEffort": "high"
    },
    "metis": {
      "model": "openai/gpt-5.3-codex",
      "reasoningEffort": "high"
    },
    "momus": {
      "model": "openai/gpt-5.3-codex",
      "reasoningEffort": "high"
    },
    "atlas": {
      "model": "openai/gpt-5.3-codex",
      "reasoningEffort": "high"
    }
  },
  "categories": {
    "visual-engineering": {
      "model": "anthropic/claude-opus-4-6",
      "reasoningEffort": "high"
    },
    "ultrabrain": {
      "model": "openai/gpt-5.3-codex",
      "reasoningEffort": "high"
    },
    "artistry": {
      "model": "openai/gpt-5.3-codex",
      "reasoningEffort": "medium"
    },
    "quick": {
      "model": "openai/gpt-5.3-codex-spark",
      "reasoningEffort": "low"
    },
    "unspecified-low": {
      "model": "openai/gpt-5.3-codex",
      "reasoningEffort": "medium"
    },
    "unspecified-high": {
      "model": "openai/gpt-5.3-codex",
      "reasoningEffort": "high"
    },
    "writing": {
      "model": "openai/gpt-5.3-codex-spark",
      "reasoningEffort": "medium"
    }
  }
}
